NEW <%= @entry_type.upcase %>:
----------------
<% @entry.class.accessible_attributes.each do |attribute| %>
<%= attribute.upcase %>:
  <%= @entry.send(attribute) %>
<% end %>
<% if @entry.person_id %>
PERSON:
<%= "#{@entry.person.first_name} #{@entry.person.last_name}" %>
<% end %>
<% if @entry.email_id %>
EMAIL:
  <%= @entry.email.address %>
<% end %>
<% if @entry.phone_number_id %>
PHONE:
  <%= @entry.phone_number.number %>
<% end %>
<% if @entry.url_id %>
URL:
  <%= @entry.url.address %>
<% end %>
<% if @entry.location_id %>
LOCATION:
  <%= @entry.location.city_state_country %>
<% end %>
